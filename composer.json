    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "enable-patching": "true",
        "composer-exit-on-patch-failure": "false",
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/google_charts": [
                "npm-asset/google-library-chart"
            ],
            "web/libraries/{$name}": [
                "harvesthq/chosen",
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches" : {
            "drupal/core": {
                "Deprecation for trim parameter": "https://www.drupal.org/files/issues/2022-03-25/Deprecated_function_trim%28%29_Passing_null_to_parameter.patch"
            },
            "drupal/entity_browser": {
                "fix issues with close": "https://www.drupal.org/files/issues/2019-08-28/close_entity_browser-2843272-13.patch"
            },
            "drupal/focal_point_focus": {
                "can't install for PHP 8": "https://git.drupalcode.org/project/focal_point_focus/-/merge_requests/1.diff",
                "svg error" : "https://www.drupal.org/files/issues/2023-01-06/3328807-focal_point-image_width_when_no_image_to_crop.patch",
                "svg error 2": "https://git.drupalcode.org/issue/focal_point-3328807/-/tree/3328807-focalpointmanagersavecropentity-argument-3"
            },
            "drupal/focal_point": {
                "cant see crosshair": "https://www.drupal.org/files/issues/2022-05-06/focal_point-migrate-3267350-28.patch"
            },
            "drupal/twig_extensions": {
                "D10 upgrade": "https://www.drupal.org/files/issues/2023-01-30/update_info_yml-3337585-14894997.patch"
            },
            "drupal/taxonomy_multidelete_terms": {
                "D10 upgrade": "https://www.drupal.org/files/issues/2023-04-13/taxonomy_multidelete_terms.1.x-dev.rector%234.patch"
            },
            "drupal/site_verify": {
                "D10 upgrade": "https://www.drupal.org/files/issues/2022-06-16/site_verify.2.0.0-alpha1.rector.patch"
            },
            "drupal/paragraphs_entity_embed": {
                "D10 upgrade": "https://www.drupal.org/files/issues/2022-07-18/paragraphs_entity_embed.2.x-dev.rector.patch"
            },
            "drupal/jquery_ui_slider": {
                "D10 upgrade": "https://www.drupal.org/files/issues/2022-06-16/jquery_ui_slider.1.x-dev.rector.patch"
            },
            "drupal/jquery_ui_button": {
                "D10 upgrade patch": "https://www.drupal.org/files/issues/2022-06-16/jquery_ui_button.1.x-dev.rector.patch"
            },
            "drupal/button_link": {
                "D10 upgrade patch": "https://www.drupal.org/files/issues/2022-06-15/button_link.2.0.x-dev.rector.patch"
            },
