{
  "name": "static",
  "version": "1.0.0",
  "description": "Static and CSS build files",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserslist": "npx browserslist@latest --update-db && echo '##########################################' && echo 'This is the list of browsers now::' && npx browserslist",
    "dev": "rimraf dist && node_modules/.bin/webpack --config config/dev.config.js || true",
    "dev-watch": "cross-env NODE_OPTIONS=--max_old_space_size=8192 node_modules/.bin/rimraf dist && node_modules/.bin/webpack --config config/dev.config.js --watch",
    "dev-serve": "cross-env NODE_OPTIONS=--max_old_space_size=8192 node_modules/.bin/webpack-dev-server --inline --hot --config config/dev.config.js --progress --colors",
    "prod": "node_modules/.bin/rimraf dist && node_modules/.bin/webpack --config config/prod.config.js --progress || true",
    "release": "node_modules/.bin/webpack --config config/release.config.js || true",
    "beautify": "./node_modules/.bin/glob-run js-beautify --replace src/**/\\*html || true",
    "eslint": "node_modules/.bin/eslint --fix 'src/scripts/**/*.js' 'src/**/*.js' || true",
    "stylelint": "npx stylelint '../web/themes/custom/toucan/css/*.css'",
    "htmllint": "node_modules/.bin/htmllint --rc ./.htmllintrc \"src/**/*.html\" || true",
    "validate-html": "node vnu-jar.js || true",
    "lint": "run-s node_modules/.bin/eslint stylelint htmllint || true",
    "format": "run-s prettier eslint beautify || true",
    "stylelint-scss": "npx stylelint ../web/themes/custom/toucan/templates/**/scss/*.scss",
    "postcss:watch": "postcss ../web/themes/custom/toucan/css/styles.css -d ../web/themes/custom/toucan/css/ --watch"
  },
  "keywords": [
    "css",
    "sass",
    "npm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.8",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "colors": "^1.4.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.13",
    "debounce": "^1.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "gsap": "^3.10.4",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "js-beautify": "^1.14.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^13.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-gyp": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.8.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.0",
    "sass": "^1.52.3",
    "svgo": "^2.8.0",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "css-has-pseudo": "^4.0.1",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0"
  }
}
